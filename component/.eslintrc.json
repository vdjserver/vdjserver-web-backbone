{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "globals": {
        "$": "readonly",
        "App": "readonly",
        "EnvironmentConfig": "readonly",
        "_": "readonly",
        "define": "readonly"
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": 0,
        "no-redeclare": 0,
        "no-prototype-builtins": 1
    }
}
